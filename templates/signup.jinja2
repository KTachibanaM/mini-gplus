{% extends "_base_guest.jinja2" %}
{% from "_form.jinja2" import field %}
{% block title %}Sign up | mini-gplus{% endblock %}
{% block content %}
<form method='post'>
    <form method='post'>
    {{ field(form.id) }}
    {{ field(form.password) }}
    {{ field(form.confirm_password) }}
    <input type='submit' value='Sign up'>
</form>
</form>
{% endblock %}
